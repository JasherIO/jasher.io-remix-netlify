    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.22",
    "typescript": "^4.1.2"
